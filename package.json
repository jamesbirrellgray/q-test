  "scripts": {
    "start": "concurrently -k \"npm:ga-start\" \"npm:be-start\"",
    "start:dev": "concurrently -k \"rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js && node ./graph-api/dist/server.js\" \"npm:be-start\"",
    "fe-start": "node front-end/index.js",
    "fe-pack": "webpack --config webpack.config.js",
    "fe-babel": "babel ./front-end/src -d ./front-end/views",
    "fe-build": "npm run fe-pack && npm run fe-babel",
    "fe-test": "echo \"Error: no test specified\" && exit 1",
    "ga-updateSchema": "babel-node ./dist/scripts/updateSchema.js",
    "ga-start": "webpack --config webpack.config.js --mode=development --graph",
    "be-start": "nodemon --watch back-end/index.js back-end/index.js"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
