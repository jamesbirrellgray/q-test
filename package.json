  "scripts": {
    "start": "concurrently -k \"npm:be-start\" \"npm:ga-start\"",
    "fe-start": "node front-end/index.js",
    "fe-pack": "webpack --config webpack.config.js",
    "fe-babel": "babel ./front-end/src -d ./front-end/views",
    "fe-build": "npm run fe-pack && npm run fe-babel",
    "fe-test": "echo \"Error: no test specified\" && exit 1",
    "ga-start": "nodemon --watch graph-api graph-api/index.js",
    "be-start": "nodemon --watch back-end/index.js back-end/index.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
